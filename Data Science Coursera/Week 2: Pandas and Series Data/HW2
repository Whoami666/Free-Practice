   "source": [
    "def proportion_of_education():\n",
    "    dic = {\"less than high school\": len(me[me==1])/len(me),\n",
    "    \"high school\": len(me[me==2])/len(me),\n",
    "    \"more than high school but not college\": len(me[me==3])/len(me),\n",
    "    \"college\": len(me[me==4])/len(me)}\n",
    "    return dic\n",
    "    raise NotImplementedError()"
   ]
